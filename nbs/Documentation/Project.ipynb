    "path = \"../../docs/general\"\n",
    "\n",
    "files = os.listdir(path)\n",
    "\n",
    "md_files = [f for f in files if \".md\" in f]\n",
    "\n",
    "for f in md_files:\n",
    "    display(Markdown(f\"{path}/{f}\"))"
   ]
